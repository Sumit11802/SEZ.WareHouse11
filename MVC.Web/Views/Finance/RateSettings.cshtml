
@{
    ViewBag.Title = "RateSettings";
    Layout = "~/Views/Shared/_MainLayout.cshtml";
}

<script src="https://cdn.datatables.net/1.13.7/js/jquery.dataTables.min.js"></script>
<link href="~/assets1/css/all.min.css" rel="stylesheet" />
<link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/6.4.2/css/all.min.css" integrity="sha512-z3gLpd7yknf1YoNbCzqRKc4qyor8gaKU1qmn+CShxbuBusANI9QpRohGBreCFkKxLhei6S9CQXFEbbKuqLg0DA==" crossorigin="anonymous" referrerpolicy="no-referrer" />

<style>

    .hiderow {
        display: none;
    }

    input::-webkit-outer-spin-button,
    input::-webkit-inner-spin-button {
        -webkit-appearance: none;
        margin: 0;
    }

    input[type=number] {
        -moz-appearance: textfield;
    }

    .close {
        color: #aaaaaa;
        float: right;
        font-size: 28px;
        font-weight: bold;
    }

        .close:hover,
        .close:focus {
            color: #000;
            text-decoration: none;
            cursor: pointer;
        }
</style>

<script src="~/js/GetTableJsonPagging.js"></script>

<div class="content-area overflow-hidden" id="divmainsummary" style="display:none;">
    <div>
        <div class="page-header">

            <div class="crumbs">
                <ul id="breadcrumbs" class="breadcrumb">
                    <li>
                        <i class="icon-home"></i>
                        <a href="index.html">Contract Setting</a>
                    </li>
                    <li class="current">
                        <a href="pages_calendar.html" title="">Contract Setting</a>
                    </li>
                </ul>
            </div>
            <div class="page-header row">
                <div class="page-title col-6">
                    <h3>Contract Setting</h3>
                </div>
                 
            </div>

        </div>
        <div class="page-body">

            <!-- Add Contact Start Model start-->
            <div class="card col-sm-12 col-lg-12">
                <div class="card-header">
                    

                    <div class="card-header-right">
                        <i class="icofont icofont-rounded-down"></i>
                        <i class="icofont icofont-refresh"></i>
                        <i class="icofont icofont-close-circled"></i>
                    </div>
                </div>


                <div class="card-block col-sm-12 col-lg-12">
                    <div class="row">
                        <div class="col-lg-4 col-sm-12">
                            <div class="form-group">
                                <label class="form-label mandatory">Contract ID <span style="color:red">*</span></label>
                       
                                    <div class="col">
                                        @Html.TextBox("TxtTraiffID", null, new { @class = "form-control", @id = "TxtTraiffID", @name = "TxtTraiffID", autocomplete = "off" })
                                    </div>
                             
                            </div>
                        </div>
                        <div class="col-md-2 col-xs-4">
                            <div class="form-group">
                                <label class="form-label mandatory" style="visibility:hidden">.</label>
                     
                                    <div class="col">
                                        <button class="btn btn-primary" data-toggle="modal" onclick="OpenTariffDetails();"><i class="fa fa-search" aria-hidden="true"></i></button>
                              
                                </div>
                            </div>
                        </div>
                        <div class="col-lg-6 col-sm-6">
                            <div class="form-group">
                                <label class="form-label mandatory">Contract Description</label>
                          
                                    <div class="col">
                                        @Html.TextBox("TxtTariffDescrribtion", null, new { @class = "form-control", @id = "TxtTariffDescrribtion", @name = "TxtTariffDescrribtion", autocomplete = "off", disabled = true })
                                    </div>
                         
                            </div>
                        </div>
                    </div>
                    <div class="row">
                        <div class="col-lg-2 col-sm-12">
                            <div class="form-group">
                                <label class="form-label">From Date  <span style="color:red">*</span></label>
                     
                                    <div class="col">
                                        @Html.TextBox("FromDate", null, new { @class = "form-control DPCostDefine", @id = "FromDate", @disabled = "disabled" })
                            
                                </div>
                            </div>
                        </div>
                        <div class="col-lg-2 col-sm-12">
                            <div class="form-group">
                                <label class="form-label"> To Date  <span style="color:red">*</span></label>
                        
                                    <div class="col">
                                        @Html.TextBox("ToDate", null, new { @class = "form-control DPCostDefine", @id = "ToDate", @disabled = "disabled" })
                                    </div>
                            
                            </div>
                        </div>
                        <div class="col-lg-3 col-sm-12">
                            <div class="form-group">
                                <label class="form-label">Invoice Type  <span style="color:red">*</span></label>
                             
                                    <div class="col">
                                        @Html.DropDownList("ddlMovementSearch", new List<SelectListItem>
                                                                                                             { new SelectListItem { Text = "--All--", Value = ""},
                                                                   new SelectListItem { Text = "Bond Ex", Value = "Bond Ex"},
                                                                   new SelectListItem { Text = "NOC", Value = "NOC"},

                                                                   new SelectListItem { Text = "Transport", Value = "Transport"},


                                                                   }, new { @class = "form-control", autocomplete = "off" })
                                    </div>
                        
                            </div>
                        </div>
                        <div class="col-lg-5 col-sm-12">
                            <div class="form-group">
                                <label class="form-label mandatory">Accounting Head  <span style="color:red">*</span></label>
                         
                                    <div class="col">
                                        @Html.DropDownList("ddlaccountingHead", (IEnumerable<SelectListItem>)ViewBag.AccountHeadList, "--Select-- ", new { @class = "form-control", @id = "ddlaccountingHead", name = "ddlaccountingHead", autocomplete = "off", onchange = "GetInvoiceTypeDetails()" })
                                    </div>
                              
                            </div>
                        </div>
                    </div>
                    <div class="row">

                        <div class="col-lg-2 col-sm-12">
                            <div class="form-group">
                                <label class="form-label"> Size  <span style="color:red">*</span></label>
                                
                                    <div class="col">
                                        @Html.DropDownList("ddlsize", new List<SelectListItem>
                                                                                    { new SelectListItem { Text = "ALL", Value = "0"},
                                            //new SelectListItem { Text = " ", Value = "0"},
                                            new SelectListItem { Text = "20", Value = "20"},
                                          new SelectListItem { Text = "40", Value = "40"},
                                           new SelectListItem { Text = "45", Value = "45"},

                                          }, new { @class = "form-control", autocomplete = "off", @id = "ddlsize" })
                                    </div>
                         
                            </div>
                        </div>
                        <div class="col-lg-2 col-sm-12">
                            <div class="form-group">
                                <label class="form-label mandatory">Cargo Type  </label>
                                
                                    <div class="col">
                                        @Html.DropDownList("ddlcargotype", (IEnumerable<SelectListItem>)ViewBag.CargoType, "ALL", new { @class = "form-control", @id = "ddlcargotype", name = "ddlcargotype", autocomplete = "off", onchange = "GetInvoiceTypeDetails()" })
                                  
                                </div>
                            </div>
                        </div>

                        <div class="col-lg-3 col-sm-12">
                            <div class="form-group">
                                <label class="form-label mandatory">Charges Based On  <span style="color:red">*</span></label>
                             
                                    <div class="col">
                                        @Html.DropDownList("ddlchargesBasedOn", (IEnumerable<SelectListItem>)ViewBag.ChagresBasedOn, "--Select-- ", new { @class = "form-control", @id = "ddlchargesBasedOn", name = "ddlchargesBasedOn", autocomplete = "off" })
                                    </div>
                             
                            </div>
                        </div>
                        <div class="col-lg-2 col-sm-12" style="display:none" id="divfixedamount">
                            <div class="form-group">
                                <label class="form-label mandatory">Fixed Amount  <span style="color:red">*</span></label>
                              
                                    <div class="col">
                                        @Html.TextBox("TxtFixedamount", null, new { @class = "form-control NonNegative", @id = "TxtFixedamount", @name = "TxtFixedamount", @tyepe = "number", autocomplete = "off" })
                                    </div>
                           
                            </div>
                        </div>
                        <div class="col-lg-2 col-sm-12" style="display:none" id="DivDays">
                            <div class="form-group">
                                <label class="form-label mandatory">Days</label>
                               
                                    <div class="col">
                                        @Html.TextBox("TXtdays", null, new { @class = "form-control NonNegative", @id = "TXtdays", @name = "TXtdays", autocomplete = "off", @tyepe = "number" })
                                    </div>
                               
                            </div>
                        </div>
                        <div class="col-lg-2 col-sm-12" style="display:none" id="divRate">
                            <div class="form-group">
                                <label class="form-label mandatory">Rate</label>
                              
                                    <div class="col">
                                        @Html.TextBox("TxtRate", null, new { @class = "form-control NonNegative", @id = "TxtRate", @name = "TxtRate", autocomplete = "off", @tyepe = "number" })
                                    </div>
                             
                            </div>
                        </div>
                        <div class="col-lg-1 col-sm-12" style="display:none" id="divslabID">
                            <div class="form-group">
                                <label class="form-label mandatory">Slab ID</label>
                          
                                    <div class="col">
                                        @Html.DropDownList("TxtslabID", (IEnumerable<SelectListItem>)ViewBag.SlabList, "--Select--", new { @class = "form-control", @id = "TxtslabID", name = "TxtslabID", autocomplete = "off", onchange = "GetSlabDetails()" })
                             
                                </div>
                            </div>
                        </div>
                        <div class="col-lg-2 col-sm-12 GA" style="display:none">
                            <div class="form-group">
                                <label class="form-label mandatory">Agreed Amount</label>
                              
                                    <div class="col">
                                        @Html.TextBox("txtAgreedAmount", null, new { @class = "form-control NonNegative", @id = "txtAgreedAmount", @name = "txtAgreedAmount", autocomplete = "off", @tyepe = "number" })
                                    </div>
                            
                            </div>
                        </div>
                        <div class="col-lg-1 col-sm-12" style="display:none" id="divslabview">
                            <div class="form-group">
                                <label class="form-label mandatory">Slab ID</label>
                          
                                    <div class="col">
                                        <button type="button" id="Add" title="search" value="search" onclick="ViewSlabs()" class="btn btn-primary btn-xs"><i class="fa fa-eye"></i></button>
                           
                                </div>
                            </div>
                        </div>
                        <div class="col-lg-1 col-sm-12" style="display:none" id="divslabbtn">
                            <div class="form-group">
                                <label class="form-label" visibility="hidden">.</label>
                             
                                    <div class="col">
                                        <button type="button" id="Add" title="Add Slab" value="search" onclick="OpenItemDetails()" class="btn btn-primary">Add Slabs</button>
                                  
                                </div>
                            </div>
                        </div>
                        <div class="col-lg-2 col-sm-12" style="display:none" id="divScanID">
                            <div class="form-group">
                                <label class="form-label mandatory">Location  <span style="color:red">*</span></label>
                           
                                    <div class="col">
                                        @Html.DropDownList("ddllocation", (IEnumerable<SelectListItem>)ViewBag.Location, "--Select-- ", new { @class = "form-control", @id = "ddllocation", name = "ddllocation", autocomplete = "off" })
                                    </div>
                        
                            </div>
                        </div>
                        <div class="col-lg-1 col-sm-12" style="display:none" id="divInsurance">
                            <div class="form-group">
                                <label class="form-label mandatory">Insurance</label>
                       
                                    <div class="col">
                                        @Html.TextBox("TxtInsurance", null, new { @class = "form-control", @id = "TxtInsurance", @name = "TxtInsurance", autocomplete = "off" })
                                    </div>
                       
                            </div>
                        </div>
                        <div class="col-lg-1 col-sm-12">
                            <div class="form-group">
                                <label class="form-label" style="visibility:hidden">.</label>
                              
                                    <div class="col">
                                        <button type="button" id="Add" title="Add" value="search" onclick="AddDetails()" class="btn btn-primary"><i class="fa fa-plus"></i></button>
                                    </div>
                                </div>
                            </div>
                        </div>

                    </div>
                    <div class="row">
                        <div class="col-lg-2 col-sm-12" style="display:none">
                            <div class="form-group">
                                <label class="form-label" style="visibility:hidden">From Date</label>
                       
                                    <div class="col">
                                        <input type="checkbox" class="checkbox origin" name="checklist1[]" id="chk1" value="true">
                                        <span>Is Tax ?</span>
                                    </div>
                          
                            </div>
                        </div>
                        <div class="col-lg-2 col-sm-12">
                            <div class="form-group">
                                <label class="form-label" style="visibility:hidden"> To Date</label>
                              
                                    <div class="col">
                                        <input type="checkbox" class="checkbox origin" name="checklist1[]" id="chk2" value="true">
                                        <span>On Deliverded ?</span>
                                    </div>
                              
                            </div>
                        </div>
                        <div class="col-lg-2 col-sm-12">
                            <div class="form-group">
                                <label class="form-label" style="visibility:hidden"> Size</label>
                            
                                    <div class="col">
                                        <input type="checkbox" class="checkbox origin" name="checklist1[]" id="chk3" value="true">
                                        <span>Consider Area ?</span>
                                    </div>
                               
                            </div>
                        </div>
                        <div class="col-lg-2 col-sm-12" style="display:none;">
                            <div class="form-group">
                                <label class="form-label" style="visibility:hidden"> ...</label>
                              
                                    <div class="col">
                                        <input type="checkbox" class="checkbox origin" name="checklist1[]" id="isInternal" value="true">
                                        <span>Is Internal ?</span>
                                    </div>
                            
                            </div>
                        </div>
                    </div>
                    <div class="row">
                        <div class="col-md-12 col-lg-12">
                            <div class="card1">
                                <div class="card-body">
                                    <div class="row">
                                        <div class="table-responsive">
                                            <table class="table table-bordered table-hover tbl-cart text-nowrap" id="exampleInv1">
                                                <thead>
                                                    <tr>
                                                        <th class="wd-10p sorting">Delete</th>
                                                        <th class="wd-10p sorting hiderow">ID</th>
                                                        <th class="wd-10p sorting hiderow">Tariff ID</th>
                                                        <th class="wd-10p sorting">Invoice Type</th>
                                                        <th class="wd-10p sorting">Account Head</th>
                                                        <th class="wd-10p sorting">From</th>
                                                        <th class="wd-10p sorting">To</th>
                                                        <th class="wd-10p sorting hiderow">Charges Head</th>
                                                        <th class="wd-10p sorting">Size</th>
                                                        <th class="wd-10p sorting hiderow">Based On</th>
                                                        <th class="wd-10p sorting">Amount</th>
                                                        <th class="wd-10p sorting">On Delivered</th>
                                                        <th class="wd-10p sorting hiderow">Location</th>
                                                        <th class="wd-10p sorting hiderow">Location Name</th>
                                                        <th class="wd-10p sorting">Consider Area</th>
                                                        <th class="wd-10p sorting hiderow">Slab Id</th>
                                                        <th class="wd-10p sorting">Cargo Type</th>
                                                        <th class="wd-10p sorting hiderow">Cargo Type ID</th>
                                                        <th class="wd-10p sorting">Internal</th>
                                                        <th class="wd-10p sorting">Agreed Amount</th>
                                                    </tr>
                                                </thead>
                                            </table>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
                <div class="card-footer ">
                    <div class="form-group">
                        <label class="form-label"></label>
                     
                            <div class="col" style="margin-right:10%">
                                <button id="btnSave" title="Save" class="btn btn-primary " onclick="return Save();" style=" margin-top: 25px;">Save</button>
                                <button id="btnSave" title="Clear" class="btn btn-danger" onclick="return cleartextboxes();" style=" margin-top: 25px;">Cancel</button>
                                @*<button class="btn btn-gray" type="button" name="Add" onclick="cleartextboxes();"><i class="fa fa-undo"></i></button>*@
                            </div>
                       
                    </div>
                </div>
            </div>
        </div>
        <br />
    </div>
 


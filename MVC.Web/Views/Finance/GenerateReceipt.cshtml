<div class="col-md-12">
    <div class="widget box">
        <div class="widget-header">
            <h4><i class="icon-reorder"></i> Generate Receipt Forms</h4>
        </div>
        <div class="widget-content">

            <div class="form-group">

                <div class="row">

                    <div class="col-md-3">
                        <label class="control-label">Receipt No</label>
                        <input type="number" id="txtReceiptNo" placeholder="Receipt Number" name="regular" class="form-control">

                    </div>
                    <div class="col-md-3">
                        <label class="control-label">Receipt Date</label>
                        <input type="datetime" id="txtReceiptDate" name="regular" class="form-control">

                    </div>
                    <div class="col-md-3">
                        <label class="control-label">Receipt Type</label>
                        <select class="form-control" id="ddlReceiptType">
                            <option value="en-EN" selected="selected">--Select--</option>
                            <option value="de-DE">Invoice Cum Receipt</option>
                            <option value="de-DE">ON A/C Receipt</option>
                            <option value="de-DE">Write Off</option>
                            <option value="de-DE">TDS</option>
                        </select>
                    </div>

                </div>


            </div>

            <div class="row">
                <div class="col-sm-6 col-lg-6">
                    <button class="btn btn-info" id="btnSave">SAVE</button>
                    <button class="btn btn-danger" id="btnCancel">CANCEL</button>
                </div>
            </div>

            @*<div class="row">
                <div class="col-md-3">
                    <label class="control-label">Proforma No</label>
